<div id="resume">
    <header id="header" class="clear">
        <div>
            <h1 class="name"><%=  theme.resume.name  %></h1>
            <h2 class="label"><%=  theme.resume.jobtitle  %></h2>
        </div>

        <span class="location">
            <% theme.resume.location.forEach(function(item) { %>
            <span class="<%=  item.key  %>"><%=  item.value  %>,</span>
            <% }) %>
        </span>


        <div id="contact">
            <% theme.resume.contact.forEach(function(item) { %>
            <div>
                <span class=" fa <%= item.icon %>"></span>
                <a target="_blank" href="<%= item.text %>"><%= item.text %></a>
            </div>
            <% }) %>
        </div>

        <div id="profiles">
            <% theme.resume.profiles.forEach(function(item) { %>
            <div class="item">
                <div class="username">
                    <span class="fa <%= item.icon %> social"></span>
                    <span class="url">
                        <a target="_blank" href="<%= item.link %>"><%= item.text %></a>
                    </span>
                </div>
            </div>
            <% }) %>
        </div>
    </header>

    <section class="section main-summary">
        <section>
            <div>
                <p><%= theme.resume.overview %></p>
            </div>
        </section>
    </section>

    <section class="section margin1">
        <header>
            <h2 class="section-title">Skills</h2>
        </header>
        <section id="skills">
            <% theme.resume.skills.forEach(function(item) { %>
            <div class="item">
                <h3 class="name">
                    <%= item.title %>
                </h3>

                <div class="level <%= item.level %>">
                    <em><%= item.level.toUpperCase() %></em>
                    <div class="bar"></div>
                </div>
                <ul class="keywords">
                    <% item.items.forEach(function(skill) { %>
                    <li><%= skill %></li>
                    <% }) %>
                </ul>
            </div>
            <% }) %>
        </section>
    </section>

    <section class="section">
        <header>
            <h2 class="section-title">Work Experience <span
                    class="item-count">(<%= theme.resume.experience.length %>)</span></h2>
        </header>

        <% theme.resume.experience.forEach(function(item, index) { %>
        <section class="work-item">

            <input id="work-item-<%=  index  %>" type="checkbox" class="toggle-item" checked="checked">
            <label for="work-item-<%=  index  %>"></label>

            <header>
                <div class="position"><%= item.position %></div>
                <div class="company"><%= item.company %></div>
                <div class="date"><%= item.duration %></div>
            </header>

            <span class="website">
                <a target="_blank" href="<%= item.website %>"><%= item.website %></a>
            </span>
            <div class="item" id="work-item">
                <div class="summary">
                    <p></p>
                    <p><%= item.description %></p>
                    <p></p>
                </div>
                <ul class="highlights">
                    <% item.hilights && item.hilights.forEach(function(hilight) { %>
                    <li>
                        <p><%= hilight %></p>
                    </li>
                    <% }) %>

                </ul>
            </div>
        </section>
        <% }) %>
    </section>

    <section class="section">
        <header>
            <h2 class="section-title">Education <span class="item-count">(1)</span></h2>
        </header>

        <section id="education">
            <% theme.resume.education.forEach(function(item) { %>
            <section class="education-item">
                <header>
                    <div class="header">
                        <div class="course">
                            <%= item.course %>
                        </div>
                        <div class="institution">
                            <%= item.institution %>
                        </div>
                        <div class="date">
                            <%= item.duration %>
                        </div>
                    </div>
                </header>

                <div class="item">
                    <div class="location">
                        <strong class="fa fa-map-marker"></strong> <span><%= item.location %></span>
                    </div>
                </div>
            </section>
            <% }) %>

        </section>
    </section>

    <section class="section">
        <header>
            <h2 class="section-title">Projects</h2>
        </header>
        <% theme.resume.projects.forEach(function(item, index) { %>
        <section id="project">
            <section class="project-item">
                <input id="project-item-<%=  index  %>" type="checkbox" class="toggle-item" checked="checked">
                <label for="project-item-<%=  index  %>"></label>

                <header>
                    <div class="header">
                        <div class="title">
                            <%= item.title %>
                        </div>
                        <div class="contribution">
                            <%= item.contribution %>
                        </div>
                        <div class="date">
                            <%= item.duration %>
                        </div>
                    </div>

                </header>

                <div class="item">
                    <div class="summary">
                        <p></p>
                        <p><%= item.description %></p>
                        <p></p>
                    </div>
                </div>
            </section>
        </section>
        <% }) %>
    </section>

    <section class="section margin1">
        <header>
            <h2 class="section-title">Languages</h2>
        </header>
        <section id="languages">
            <% theme.resume.languages.forEach(function(item) { %>
            <div class="display">
                <h3 class="language">
                    <%= item.name %>
                </h3>
                <div class="item">
                    <div class="level fluency <%= item.proficiency %>">
                        <em><%= item.proficiency.toUpperCase() %></em>
                        <div class="bar"></div>
                    </div>
                </div>
            </div>
            <% }) %>
        </section>
    </section>

</div>